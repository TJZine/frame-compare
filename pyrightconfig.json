{
  "typeCheckingMode": "standard",
  "include": ["src", "tests", "frame_compare.py", "typings"],
  "strict": ["src/frame_compare"],
  "stubPath": "typings",
  "exclude": [".venv/**", "build/**", "dist/**", "**/__pycache__/**"],
  "venvPath": ".",
  "venv": ".venv",
  "executionEnvironments": [
    { "root": "src/frame_compare", "extraPaths": [".", "src", "typings"] },
    { "root": "src", "extraPaths": [".", "src", "typings"] },
    { "root": "tests", "extraPaths": [".", "src", "typings"] },
    { "root": ".", "extraPaths": [".", "src", "typings"] }
  ],
  "reportMissingImports": "error",
  "reportUnknownMemberType": "warning",
  "reportOptionalMemberAccess": "error"
}
